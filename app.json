{
  "expo": {
    "name": "Dominicana",
    "slug": "dominicana",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dominicana",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#8B0000"
    },
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to show nearby Dominican provinces and communities."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#8B0000"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD7IgMJPbxOyNigm9U8dj5_6wozeQN3Jpo"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-file-system",
      "expo-document-picker",
      "expo-av",
      "expo-calendar",
      "expo-location",
      ["expo-font",
      {
        "android": {
          "fonts": [
              {
                "fontFamily": "Neuton",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Neuton/Neuton-Regular.ttf",
                    "weight": 400,
                    "style": "normal"
                  },
                  {
                    "path": "./assets/fonts/Neuton/Neuton-Bold.ttf",
                    "weight": 700
                  },
                  {
                    "path": "./assets/fonts/Neuton/Neuton-Italic.ttf",
                    "weight": 400,
                    "style": "italic"
                  },
                  {
                    "path": "./assets/fonts/Neuton/Neuton-Light.ttf",
                    "weight": 300,
                    "style": "normal"
                  }
                ]
              }
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
